  after  operationEnd(boolean ) { //+
	synchronized (this) {
	    if ((deleteInfo != null) && (deleteInfo.size() > 0)) {
		envImpl.addToCompressorQueue(deleteInfo.values(), false);
		deleteInfo.clear();
	    }
	}
    }

}
