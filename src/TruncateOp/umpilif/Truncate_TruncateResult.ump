namespace com.sleepycat.je.dbi;

class TruncateResult
{
  depend de.ovgu.cide.jakutil.*;
    private DatabaseImpl db;

    private int count;

    TruncateResult(DatabaseImpl db, int count) {
			this.db = db;
			this.count = count;
    }

    public DatabaseImpl getDatabase() {
			return db;
    }

    public int getRecordCount() {
			return count;
    }

}
