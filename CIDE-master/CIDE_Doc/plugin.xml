<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="de.ovgu.cide.doc.helpextension"
         name="Help Contents"
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true">
      </toc>
   </extension>
   <extension
         id="de.ovgu.cide.doc.welcomeextension"
         name="Welcome Extension"
         point="org.eclipse.ui.intro.configExtension">
      <configExtension
            configId="org.eclipse.ui.intro.universalConfig"
            content="welcome/overview.xml">
      </configExtension>
      <configExtension
            configId="org.eclipse.ui.intro.universalConfig"
            content="welcome/tutorial.xml">
      </configExtension>
   </extension>
   <extension
         id="de.ovgu.cide.doc.sheetcheatextension"
         name="Sheet Cheats Extension"
         point="org.eclipse.ui.cheatsheets.cheatSheetContent">
      <cheatsheet
            category="de.ovgu.cide.doc.cheatsheet"
            composite="false"
            contentFile="cheatsheets/HelloWorld.xml"
            id="de.ovgu.cide.doc.cheatsheet.helloworld"
            name="Hello World">
      </cheatsheet>
      <category
            id="de.ovgu.cide.doc.cheatsheet"
            name="CIDE">
      </category>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>

</plugin>
