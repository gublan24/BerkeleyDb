
namespace com.sleepycat.je.util;
class DbCacheSize {

      after Label833:  insertRecords(PrintStream , Environment , Database , long , int , int , boolean ) {
      	stats = env.getStats(null);
    		if (stats.getNNodesScanned() > 0) {
                    out.println("*** Ran out of cache memory at record " + i +
                                " -- try increasing the Java heap size ***");
                    return;
        }

      }
    
}


   
